generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model EarningsCalendar {
  id    Int     @default(autoincrement()) @id
  symbol String
  date DateTime
  epsActual Int
  epsEstimate Int
  hour String
  quarter Int
  revenueActual BigInt
  revenueEstimate BigInt
  year Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

